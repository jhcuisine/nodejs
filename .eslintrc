{
  "extends": [
    "prettier",
    "plugin:prettier/recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "no-var": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": false,
        "printWidth": 100,
        "tabWidth": 2,
        "semi": true,
        "endOfLine": "auto",
        "arrowParens": "always"
      }
    ]
  },
  "plugins": ["prettier"]
}
